{
  "name": "todo.tab",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Keep a list of things on the new tab page",
  "homepage_url": "https://github.com/mcgraths/todo.tab",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "options_page": "src/options_custom/index.html",
  "chrome_url_overrides": {
    "newtab": "src/override/override.html"
  },
  "permissions": [
    "clipboardRead",
    "contextMenus",
    "management",
    "storage"
  ],
  "omnibox": {
    "keyword": "todo"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}